            </Label.Effect>
        </Label>
        <Label x:Name="CurrentVersion" Content="Checking" Margin="92,0,20,9" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Bold" FontSize="14" Grid.Column="3" Grid.Row="11">
            <Label.Effect>
                <DropShadowEffect
                    ShadowDepth="0"
