                    BlurRadius="2"/>
            </Label.Effect>
        </Label>
        <Label x:Name="VersionCheck" Content="©2022 YAG-dev" Margin="80,0,10,10" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Bold" FontSize="14" Grid.ColumnSpan="2" Grid.Column="1" Grid.Row="2">
            <Label.Effect>
                <DropShadowEffect
                    ShadowDepth="0"
                    Direction="0"
                    Color="{DynamicResource SystemAltHighColor}"
                    Opacity="0.8"
                    BlurRadius="2"/>
            </Label.Effect>
        </Label>
        <Image Margin="10,10,0,10" Source="TT21.9.1Logo.png" Stretch="Fill" Height="70" VerticalAlignment="Center" HorizontalAlignment="Left" Width="70" Grid.Column="1" Grid.Row="1" Grid.RowSpan="2"/>
        <Button x:Name="DeviceSpecificFeatures_Copy3" Content="Launch" Margin="0,0,1,0" Background="{DynamicResource SystemBaseHighColorBrush}" Foreground="{DynamicResource SystemBaseHighColorBrush}" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Semibold" FontSize="16" Style="{DynamicResource ImageButton22.1}" Click="BackButton_Click" Grid.Column="1" Grid.Row="4" Grid.RowSpan="2" >
            <Button.Effect>
                <DropShadowEffect
                    ShadowDepth="0"
                    Direction="0"
                    Color="{DynamicResource SystemBaseHighColor}"
