            </Button.Effect>
        </Button>
        <Image x:Name="BtnImg_Copy" Margin="11,10,0,10" Source="tt-launch-dark.png" Stretch="Fill" Height="70" VerticalAlignment="Center" HorizontalAlignment="Left" Width="70" Grid.RowSpan="2" Grid.Row="4" Grid.Column="2"/>
        <Button x:Name="DeviceSpecificFeatures_Copy" Content="Reinstall" Margin="0,0,1,0" Background="{DynamicResource SystemBaseHighColorBrush}" Foreground="{DynamicResource SystemBaseHighColorBrush}" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Semibold" FontSize="16" Style="{DynamicResource ImageButton22.1}" Click="Reinstall" Grid.Column="1" Grid.Row="7" Grid.RowSpan="2" >
            <Button.Effect>
                <DropShadowEffect
