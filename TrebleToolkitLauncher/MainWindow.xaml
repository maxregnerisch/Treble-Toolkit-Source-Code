<Window x:Class="TrebleToolkitLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrebleToolkitLauncher"
        xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
        mc:Ignorable="d"
        Title="Treble Toolkit" Height="600" Width="1000" 
        MinWidth="1000" MinHeight="600" fw:AcrylicWindow.Enabled="True" fw:AcrylicWindow.NoiseOpacity="0" fw:AcrylicPanel.NoiseOpacity="0" BorderThickness="0" >
    <Grid x:Name="GridMain" Background="{DynamicResource DynamicResource SystemBaseLowColorBrush}">
        <ScrollViewer x:Name="scrollviewer" ClipToBounds="True" VerticalScrollBarVisibility="Hidden">
            <Frame x:Name="ContentFrame" Source="HomePage.xaml" NavigationUIVisibility="Hidden" ScrollViewer.CanContentScroll="True" >
            </Frame>
        </ScrollViewer>
    </Grid>
</Window>
