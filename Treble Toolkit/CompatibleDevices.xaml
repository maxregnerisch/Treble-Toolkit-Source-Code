        <Button x:Name="OP8_Copy3" Content="Button" Margin="1,152.417,0,100" Grid.Row="2" VerticalAlignment="Bottom" Height="48" Opacity="0" Click="A1_Click" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Button x:Name="OP8_Copy4" Content="Button" Margin="1,202.417,0,50" Grid.Row="2" VerticalAlignment="Bottom" Height="48" Opacity="0" Click="HM10L_Click" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Button x:Name="OP8_Copy5" Content="Button" Margin="0,252.417,0,0" Grid.Row="2" VerticalAlignment="Bottom" Height="48" Opacity="0" Click="BACK_Click" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label x:Name="Title" Content="Compatible Devices" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="/gui;component/Fonts/#Bahnschrift Semibold" FontSize="20" Grid.ColumnSpan="4" Height="30" VerticalAlignment="Top"/>

    </Grid>
</Page>
