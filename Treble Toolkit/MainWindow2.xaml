<Window x:Class="Treble_Toolkit.MainWindow2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Treble_Toolkit"
        xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
        mc:Ignorable="d"
        Title="Treble Toolkit" Height="600" Width="800" 
        MinWidth="800" MinHeight="600" fw:AcrylicWindow.Enabled="False" Background="{DynamicResource SystemAltMediumHighColorBrush}" BorderThickness="0">
    <Grid x:Name="GridMain">
        <ScrollViewer x:Name="scrollviewer" ClipToBounds="True" VerticalScrollBarVisibility="Hidden">
            <Frame x:Name="ContentFrame" Source="HomeScreen.xaml" NavigationUIVisibility="Hidden" ScrollViewer.CanContentScroll="True" >
            </Frame>
        </ScrollViewer>
    </Grid>
</Window>
