<Window x:Class="TrebleToolkitUpdaterLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:fw="clr-namespace:SourceChord.FluentWPF;assembly=FluentWPF"
        xmlns:local="clr-namespace:TrebleToolkitUpdaterLauncher"
        mc:Ignorable="d"
        Title="Treble Toolkit Launcher" Height="340" Width="850" Background="{DynamicResource SystemAltMediumHighColorBrush}" xmlns:m="clr-namespace:MultiLanguageMarkup" MinWidth="850"
        fw:AcrylicWindow.Enabled="True" MinHeight="340" >
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="225*"/>
            <ColumnDefinition Width="305*"/>
            <ColumnDefinition Width="312*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="LaunchStuffs" Content="Launch" Margin="8,60,2,186" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" FontSize="26" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Semibold" />
        <Label x:Name="Troubleshooting" Content="Troubleshooting" Margin="8,115,2,131" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" FontSize="26" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Semibold" />
        <Label x:Name="Launcher" Content="Launcher" Margin="8,170,2,76" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" FontSize="26" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Semibold" />
        <Label x:Name="status_lbl" Content="©2021 YAG-dev" Margin="10,0,10,8" VerticalAlignment="Bottom" Height="38" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Semibold" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Grid.ColumnSpan="3" />
        <Label x:Name="Title" Content="Welcome to Treble Toolkit Launcher" Margin="10,10,10,0" VerticalAlignment="Top" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" FontSize="24" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="/TrebleToolkitLauncher;component/Fonts/#Bahnschrift Semibold" Grid.ColumnSpan="3" />
        <ProgressBar x:Name="status_pgr" Height="20" Margin="10,0,10,51" VerticalAlignment="Bottom" Background="{x:Null}" Foreground="{DynamicResource SystemBaseHighColorBrush}" BorderBrush="{DynamicResource SystemBaseHighColorBrush}" RenderTransformOrigin="0.5,0.5" BorderThickness="3" UseLayoutRounding="False" Grid.ColumnSpan="3" Visibility="Hidden"/>
        <Rectangle x:Name="Launch2Rectangle" Fill="{DynamicResource SystemBaseLowColorBrush}" Height="50" Margin="2,60,10,186" Stroke="{x:Static fw:AccentColors.ImmersiveSystemAccentBrush}" VerticalAlignment="Center" RadiusY="9.5" RadiusX="9.5" StrokeThickness="2" Grid.Column="2"/>
        <Rectangle x:Name="LaunchRectangle" Fill="{DynamicResource SystemBaseLowColorBrush}" Height="50" Margin="3,60,2,186" Stroke="{x:Static fw:AccentColors.ImmersiveSystemAccentBrush}" VerticalAlignment="Center" RadiusY="9.5" RadiusX="9.5" StrokeThickness="2" Grid.Column="1"/>
        <Label x:Name="Launch2Label" Content="Launch (Offline)" Margin="2,60,10,186" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="Bahnschrift Semibold" FontSize="16" Grid.Column="2"/>
        <Label x:Name="LaunchLabel" Content="Launch" Margin="3,60,2,186" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="Bahnschrift Semibold" FontSize="16" Grid.Column="1"/>
        <Rectangle x:Name="UpdateRectangle" Fill="{DynamicResource SystemBaseLowColorBrush}" Height="50" Margin="3,170,10,76" Stroke="{x:Static fw:AccentColors.ImmersiveSystemAccentBrush}" VerticalAlignment="Center" RadiusY="9.5" RadiusX="9.5" StrokeThickness="2" Grid.ColumnSpan="2" Grid.Column="1"/>
        <Label x:Name="UpdateLabel" Content="Update Launcher" Margin="3,170,10,76" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="Bahnschrift Semibold" FontSize="16" Grid.ColumnSpan="2" Grid.Column="1"/>
        <Rectangle x:Name="CheckConnectionRectangle" Fill="{DynamicResource SystemBaseLowColorBrush}" Height="50" Margin="2,115,10,131" Stroke="{x:Static fw:AccentColors.ImmersiveSystemAccentBrush}" VerticalAlignment="Center" RadiusY="9.5" RadiusX="9.5" StrokeThickness="2" Grid.Column="2"/>
        <Rectangle x:Name="ReinstallRectangle" Fill="{DynamicResource SystemBaseLowColorBrush}" Height="50" Margin="3,115,2,131" Stroke="{x:Static fw:AccentColors.ImmersiveSystemAccentBrush}" VerticalAlignment="Center" RadiusY="9.5" RadiusX="9.5" StrokeThickness="2" Grid.Column="1"/>
        <Label x:Name="CheckLabel" Content="Check Connection" Margin="2,115,10,131" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="Bahnschrift Semibold" FontSize="16" Grid.Column="2"/>
        <Label x:Name="ReinstallLabel" Content="Force Reinstall" Margin="3,115,2,131" VerticalAlignment="Center" Height="50" Foreground="{DynamicResource SystemBaseHighColorBrush}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="Bahnschrift Semibold" FontSize="16" Grid.Column="1"/>
        <Button x:Name="Launch" Content="Button" Margin="3,60,2,186" VerticalAlignment="Center" Height="50" Background="#FFDDDDDD" Opacity="0" Click="Button_Click" Grid.Column="1" />
        <Button x:Name="Launch2" Content="Button" Margin="2,60,10,186" VerticalAlignment="Center" Height="50" Background="#FFDDDDDD" Opacity="0" Click="Button_Click2" Grid.Column="2" />
        <Button x:Name="Reinstall" Content="Button" Margin="3,115,2,131" VerticalAlignment="Center" Height="50" Background="#FFDDDDDD" Opacity="0" Click="Button_Click1" Grid.Column="1" />
        <Button x:Name="CheckConnection" Content="Button" Margin="2,115,10,131" VerticalAlignment="Center" Height="50" Background="#FFDDDDDD" Opacity="0" Click="CheckConnection_Click" Grid.Column="2" />
        <Button x:Name="UpdateLauncher" Content="Button" Margin="3,170,10,76" VerticalAlignment="Center" Height="50" Background="#FFDDDDDD" Opacity="0" Click="Button_ClickL" Grid.ColumnSpan="2" Grid.Column="1" />
    </Grid>
</Window>
